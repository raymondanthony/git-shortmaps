a _git_add :add
A _git_add :add -A
c _git_commit :commit
C _git_commit :commit -am
co _git_checkout :checkout
d _git_diff :diff
f _git_fetch :fetch
m _git_merge :merge
p _git_push :push
P _git_pull :pull
s : :status
S _git_stash :stash
t : tig
T _git_tag :tag
- : :checkout -
-- : |cd "$( git rev-parse --git-dir 2>/dev/null )/../"
